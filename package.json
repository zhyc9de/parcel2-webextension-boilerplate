    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "parcel": "2.0.0-beta.2",
    "typescript": "^4.2.3"
  },
  "targets": {
